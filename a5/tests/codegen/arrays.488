{

  var i,j:integer
  var a[5]:integer
  var b[-1..3]:integer
  %var c[10,3]:integer


  i := 1
  while i<6 do {
  	a[i] := i
  	b[i-2] := i
  	put "a[", i, "] = ", a[i], newline
  	put "b[", i-2, "] = ", b[i-2], newline
  	i := i + 1
  } end
  
  put newline
  
  i := 1
  while i<6 do {
  	a[i] := a[i] + 1
  	put "a[", i, "] = ", a[i], newline
  	i := i + 1
  } end
  
  put newline
  
  i := -1
  while i<4 do {
  	b[i] := b[i] + 2
  	put "b[", i, "] = ", b[i], newline
  	i := i + 1
  } end


}
