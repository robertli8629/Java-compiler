{ 
  var a:integer
  var ans:integer
  var i:integer
  
  i := 1
  ans := 0
  a := 1
  while true
  do
    {
      ans := ans + a
      a := a + 1
      exit when a>100
    }
  end
  if(ans=5050) then{
    put "result:1"
  }else{
    put "result:0"
  }
  fi
  
  put newline
  i:=i+1
  ans:=0
  a:=1
  while a<101
  do
    {
      ans:=ans+a
      a:=a+1
    }
  end
  if (ans = 5050) then{
    put "result:1"
  } else {
    put "result:0"
  }
  fi
  
} 
