{


  var x:integer

  x := 10/(2+1)
  x := (not false?-5:6)
  if (true not= false) then {
      x := 3
  } else {
      x := 4
  }
  fi
  put "x: ", x, newline, newline
  
  x := 0
  while (x<10) do {
      x := x+1
  } end
  
  put "x: ", x, newline, newline
  
  x := 0
  repeat {
      x := x+1
  } until (x=10)
  
  put "x: ", x, newline, newline

}
