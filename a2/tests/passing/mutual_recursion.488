{
% Forward declartions and mutually recursive procedures
% May only need to forward declare proc2

%forward proc proc1(i : integer)
%forward proc proc2(i : integer)

proc proc1(i : integer) {
    if (i >= 10)
    then {
        return
    } else {
        proc2(i + 1)
    }
    fi
}

proc proc2(i : integer) {
    if (i >= 10)
    then {
        return
    } else {
        proc1(i + 1)
    }
    fi
}

proc1(0)

}
