{

	% CSC488 A1 part d
    % A program using non-recursive functions and procedures with and
    % without parameters.
    % Author: g1liyuch

	func foo():integer {
		result 0
	}

	func add(x:integer, y:integer):integer {
		result x+y
	}

	proc print_zero() {
		put 0
		return
	}

	proc print_int(x:integer) {
		put x
		return
	}

	var a:integer
	var b:integer

	a := foo()
	b := add(a, 1)
	print_zero()
	print_int(a+b)

}