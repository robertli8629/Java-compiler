{
func foo():integer {
	return 0
}

func add_x_y_times(x:integer, y:integer):integer {
	if (x=0) then return x
	add_x_y_times(x+1,y-1)
}

proc print_zero() {
	put 0
}

proc print_int_lines(x:integer) {
	if (x=0) then return
	put x
	print_int_lines(x-1)
}

var a:integer
var b:integer

a = foo()
b = add_x_y_times(a, 1)
print_zero()
print_int_lines(a+b)

}